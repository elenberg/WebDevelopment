<html>
<head>
  <% include ../pages/head %>
  <% include ../pages/header %>
  <title>"Dashboard"</title>
</head>

<body class ="container">
  <main>
    <div class="container">
      <div id="grid">
       <% gram.forEach(function(gram){ %>
          <div class="box">
            <div class="instagram-pic col-sm-3">
              <a href="<%= gram.link %>" target= "_blank">
                 <img src="<%= gram.images.standard_resolution.url %>" class= "img-responsive">
              </a>
              <button class="pull--left"><%= gram.comments.count %> comments</button>
              <button class="pull--right"><%= gram.likes.count %> likes</button>
            </div>
          </div>
        <% }); %>
      </div>
    </div>
  </main>
</body>
</html>
